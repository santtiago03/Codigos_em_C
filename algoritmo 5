#include <stdio.h>
int main() {
	float x,y,z;
	  printf("Insira um numero :");	
	  scanf ("%f",&x);
		    printf("Insira outro numero :");
		    scanf("%f",&y);
        		printf("Insira outro numero");
			        scanf("%f",&z);
		
float media = (x+y+z)/2;
printf ("A media dos numeros e :%f\n", media);
return 0;
}				
